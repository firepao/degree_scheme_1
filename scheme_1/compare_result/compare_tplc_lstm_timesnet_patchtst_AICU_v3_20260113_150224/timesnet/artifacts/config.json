{
  "model_name": "timesnet",
  "train": {
    "epochs": 20,
    "lr": 0.001,
    "weight_decay": 0.0,
    "device": "cuda",
    "grad_clip_max_norm": 1.0,
    "use_amp": false,
    "lr_scheduler": "plateau",
    "early_stop_patience": 6
  },
  "timesnet_arch": {
    "d_model": 32,
    "d_ff": 64,
    "e_layers": 2,
    "top_k": 5,
    "num_kernels": 6,
    "dropout": 0.1
  }
}